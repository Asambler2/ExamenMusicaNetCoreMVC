@model IEnumerable<ExamenMusicaNetCoreMVC.Models.ConciertosGrupo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table" id="tableData">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Conciertos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Grupos)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                    @Html.DisplayFor(modelItem => item.Conciertos.Titulo)-@Html.DisplayFor(modelItem => item.Conciertos.Id)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.Grupos.Nombre)-@Html.DisplayFor(modelItem => item.Grupos.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
    <ul class="pagination pagination-lg pager" id="myPager"></ul>
</table>
